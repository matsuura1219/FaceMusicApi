<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetMusicForEmotions</servlet-name>
  	<display-name>GetMusicForEmotions</display-name>
  	<description></description>
  	<servlet-class>controller.GetMusicForEmotions</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetMusicForAges</servlet-name>
  	<display-name>GetMusicForAges</display-name>
  	<description></description>
  	<servlet-class>controller.GetMusicForAges</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetMusicForEmotions</servlet-name>
  	<url-pattern>/EmotionApi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetMusicForAges</servlet-name>
  	<url-pattern>/AgeApi</url-pattern>
  </servlet-mapping>
</web-app>
